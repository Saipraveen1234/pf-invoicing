{
    "version": 2,
    "framework": "angular",
    "outputDirectory": "frontend/dist/frontend/browser",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/.*|assets/|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}